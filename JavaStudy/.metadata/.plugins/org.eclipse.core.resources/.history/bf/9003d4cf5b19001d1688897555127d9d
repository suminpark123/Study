package java0811;

import java.util.Scanner;

public class Ex09_보충3번 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		System.out.println("숫자 입력:" );
		int i=sc.nextInt();
		
		int x=i%10;
				
		System.out.println("반올림 수 :"+x);
	}

}
